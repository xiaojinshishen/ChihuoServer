        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.model">

 <class name="UserLabel" table="user_label">
        <id name="user_id" />
        
        <property name="user_label1"/>
        <property name="user_label2"/>
        <property name="user_label3" />
        <property name="user_label4"/>
        <property name="user_label5"/>
