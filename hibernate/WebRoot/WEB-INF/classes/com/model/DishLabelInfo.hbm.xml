<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.model">

 <class name="DishLabelInfo" table="dish_label_info">
 
   <composite-id>
        <key-property name="dish_id"/>
        <key-property name="dish_label"/>
   </composite-id>
        
   <property name="dish_label_count"/>
   <property name="dish_label_score"/>
   <property name="dish_label_average"/>
   <property name="date_time"/>
        
        
        
    </class>

</hibernate-mapping>